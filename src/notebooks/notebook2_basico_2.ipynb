    "    tem_descriptions = estrutura['Descriptions'] > 0\n",
    "    \n",
    "    qualidade_items = [\n",
    "        (\"Items presentes\", tem_items),\n",
    "        (\"T√≠tulos presentes\", tem_titles),\n",
    "        (\"Links presentes\", tem_links),\n",
    "        (\"Descri√ß√µes presentes\", tem_descriptions),\n",
    "        (\"RSS bem formado\", estrutura['RSS Root'] > 0),\n",
    "    ]\n",
    "    \n",
    "    for descricao, status in qualidade_items:\n",
    "        icone = \"‚úÖ\" if status else \"‚ùå\"\n",
    "        print(f\"   {icone} {descricao}\")\n",
    "    \n",
    "    # Score de qualidade\n",
    "    score = sum(1 for _, status in qualidade_items if status)\n",
    "    max_score = len(qualidade_items)\n",
    "    percentual = (score / max_score) * 100\n",
    "    \n",
    "    print(f\"\\nüèÜ SCORE DE QUALIDADE: {score}/{max_score} ({percentual:.1f}%)\")\n",
    "    \n",
    "    if percentual >= 80:\n",
    "        print(\"   üåü Excelente! RSS com √≥tima estrutura\")\n",
    "    elif percentual >= 60:\n",
    "        print(\"   üëç Bom! RSS com estrutura adequada\")\n",
    "    else:\n",
    "        print(\"   ‚ö†Ô∏è RSS com poss√≠veis problemas de estrutura\")\n",
    "\n",
    "# Criar relat√≥rio visual\n",
    "criar_relatorio_visual(estrutura, info)"
