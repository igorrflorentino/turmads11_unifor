    "def extrair_amostras_rss(conteudo):\n",
    "    \"\"\"\n",
    "    Extrai amostras espec√≠ficas do conte√∫do RSS\n",
    "    \"\"\"\n",
    "    print(\"üìã AMOSTRAS DO CONTE√öDO RSS\")\n",
    "    print(\"=\" * 40)\n",
    "    \n",
    "    # 1. Cabe√ßalho XML e RSS\n",
    "    print(\"1Ô∏è‚É£ CABE√áALHO XML/RSS (primeiras 10 linhas):\")\n",
    "    print(\"-\" * 40)\n",
    "    linhas = conteudo.splitlines()\n",
    "    for i, linha in enumerate(linhas[:10], 1):\n",
    "        print(f\"{i:2d}: {linha}\")\n",
    "    \n",
    "    # 2. Informa√ß√µes do Canal\n",
    "    print(\"\\n2Ô∏è‚É£ INFORMA√á√ïES DO CANAL:\")\n",
    "    print(\"-\" * 40)\n",
    "    \n",
    "    # Extrair t√≠tulo do canal\n",
    "    inicio_channel = conteudo.find('<channel>')\n",
    "    primeiro_item = conteudo.find('<item>')\n",
    "    \n",
    "    if inicio_channel != -1 and primeiro_item != -1:\n",
    "        info_canal = conteudo[inicio_channel:primeiro_item]\n",
    "        \n",
    "        # Buscar elementos espec√≠ficos\n",
    "        elementos_canal = ['title', 'link', 'description', 'language', 'lastBuildDate']\n",
    "        \n",
    "        for elemento in elementos_canal:\n",
    "            inicio = info_canal.find(f'<{elemento}>')\n",
    "            fim = info_canal.find(f'</{elemento}>')\n",
    "            \n",
    "            if inicio != -1 and fim != -1:\n",
    "                valor = info_canal[inicio + len(elemento) + 2:fim]\n",
    "                print(f\"   üìã {elemento.capitalize()}: {valor[:100]}{'...' if len(valor) > 100 else ''}\")\n",
    "    \n",
    "    # 3. Primeiro item completo\n",
    "    print(\"\\n3Ô∏è‚É£ PRIMEIRO ITEM COMPLETO:\")\n",
    "    print(\"-\" * 40)\n",
    "    \n",
    "    primeiro_item_inicio = conteudo.find('<item>')\n",
    "    primeiro_item_fim = conteudo.find('</item>') + 7\n",
    "    \n",
    "    if primeiro_item_inicio != -1 and primeiro_item_fim != -1:\n",
    "        primeiro_item = conteudo[primeiro_item_inicio:primeiro_item_fim]\n",
    "        \n",
    "        # Formatar para melhor visualiza√ß√£o\n",
    "        linhas_item = primeiro_item.split('>')\n",
    "        for linha in linhas_item[:15]:  # Primeiras 15 partes\n",
    "            if linha.strip():\n",
    "                print(f\"   {linha.strip()}>\")\n",
    "        \n",
    "        if len(linhas_item) > 15:\n",
    "            print(\"   ... (item truncado para visualiza√ß√£o)\")\n",
    "    \n",
    "    return True\n",
    "\n",
    "# Executar extra√ß√£o de amostras\n",
    "extrair_amostras_rss(conteudo)"
   ]
